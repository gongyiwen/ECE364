class PointND:
    def __init__(self,*args):
        self.t=args
        self.n=len(args)
        for i in args:
            if type(i) is not float:
                raise ValueError #"Cannot instantiate an object with non-float values.\""

    def __str__(self):
        temp=[]
        for i in self.t:
            j=format(i,'.2f')
            temp.append(j)
        s=', '.join(str(k) for k in temp)
        s="("+s+")"
        return s

    def __hash__(self):
        return hash(self.t)

    def distanceFrom(self, other):
        if other.n != self.n:
            raise ValueError
        sum=0
        for i,j in zip(self.t,other.t):
            sum+=pow((i-j),2)
        sum=pow(sum,0.5)
        return sum

    def nearestPoint(self, points):
        if points == []:
            raise ValueError
        sum=0
        p=PointND(0.0,0.0)
        smallest=100000000
        for i in points:
            for j,k in zip(self.t,i.t):
                sum+=pow((j-k),2)
            sum=pow(sum,0.5)
            if sum<smallest:
                smallest=sum
                p=i
        return p

    def clone(self):
        s=copy.copy(self)
        return s

