class PointSet:
    def __init__(self,**kwargs):
        if not kwargs:
            self.points=0
            self.n=0
        else:
            if "pointList" in kwargs:
                pointList=kwargs["pointList"]
                if not pointList:
                    raise ValueError
                else:
                    self.n=pointList[0].n
                    self.points=set(pointList)
                    for i in self.points:
                        if i.n != self.n:
                            raise ValueError
            else:
                raise KeyError

    def addPoint(self,p):
        if p.n != self.n:
            raise ValueError
        else:
            self.points.add(p)

    def count(self):
        return len(self.points)

    def computeBoundingHyperCube(self):
        max=[]
        min=[]
        for i in range(self.n):
            a=[]
            for j in self.points:
                b=j[i]
                a.append(b)
            a.sort()
            print(a)
            min.append(a[0])
            max.append(a[self.count()-1])
        A=tuple(min)
        B=tuple(max)
        minpoint=PointND(*A)
        maxpoint=PointND(*B)
        c=[]
        c.append(minpoint)
        c.append(maxpoint)
        C=tuple(c)
        return C


    def computeNearestNeighbors(self,otherPointSet):
        b=[]
        for i in self.points:
            j=i.nearestPoint(otherPointSet.points)
            a=[]
            a.append(i)
            a.append(j)
            A=tuple(a)
            b.append(A)
        b.sort()
        return b


    def __add__(self, other):
        if other.n != self.n:
            raise ValueError
        else:
            self.addPoint(other)
            return self

    def __sub__(self, other):
        if other in self.points:
            self.points.remove(other)
            return self
        return self

    def __contains__(self, item):
        if item in self.points:
            return True
        else:
            return False






































