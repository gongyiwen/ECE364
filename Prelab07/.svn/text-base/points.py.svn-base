    def __mul__(self,other):
        if type(other) is float:
             sum=0
             k=[]
             for i in self.t:
                sum=i*other
                k.append(sum)
             temp=tuple(k)
             return PointND(*temp)

    def __rmul__(self, other):
        return self.__mul__(other)

    def __truediv__(self,other):

        if type(other) is float:
             sum=0
             k=[]
             for i in self.t:
                sum=i/other
                k.append(sum)
             temp=tuple(k)
             return PointND(*temp)

        if type(self) is float:
             sum=0
             k=[]
             for i in other.t:
                sum=i/self
                k.append(sum)
             temp=tuple(k)
             return PointND(*temp)

    def __neg__(self):
        sum=0
        k=[]
        for i in self.t:
            sum=-i
            k.append(sum)
        temp=tuple(k)
        return PointND(*temp)

    def __getitem__(self,item):
        ind=0
        for i in self.t:
            if ind == item:
                return i
            ind+=1

    def __lt__(self, other): #<
        if self.n != other.n:
            raise ValueError
        origin=self.__mul__(0.0)
        i=self.distanceFrom(origin)
        j=other.distanceFrom(origin)
        if i < j:
            return True
        else:
            return False

    def __gt__(self, other): #>
        if self.n != other.n:
            raise ValueError
        origin=self.__mul__(0.0)
        i=self.distanceFrom(origin)
        print(i)
        j=other.distanceFrom(origin)
        print(j)
        if i > j:
            return True
        else:
            return False

    def __ge__(self, other):#>=
        if self.n != other.n:
            raise ValueError
        origin=self.__mul__(0.0)

        i=self.distanceFrom(origin)
        j=other.distanceFrom(origin)
        if i >= j:
            return True
        else:
            return False

    def __le__(self, other):#<=
        if self.n != other.n:
            raise ValueError
        origin=self.__mul__(0.0)
        i=self.distanceFrom(origin)
        j=other.distanceFrom(origin)
        if i <= j:
            return True
        else:
            return False

    def __eq__(self, other):#=
        if other.n != self.n:
                 raise ValueError
        else:
            for i,j in zip(self.t,other.t):
                if i!=j:
                    return False
            return True

    def __ne__(self, other):#!=
         if other.n != self.n:
                 raise ValueError
         else:
            for i,j in zip(self.t,other.t):
                if i==j:
                    return False
            return True

